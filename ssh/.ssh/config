Host *
    AddKeysToAgent yes
    ForwardAgent yes
    ServerAliveInterval 60

Host gwen
    Hostname gwen
    IdentityFile ~/.ssh/grey
    User Kevin

Host grey
    Hostname grey.local
    IdentityFile ~/.ssh/gwen
    User kevin

Host github.com
    IdentityFile ~/.ssh/github

Host aur.archlinux.org
    IdentityFile ~/.ssh/aur
